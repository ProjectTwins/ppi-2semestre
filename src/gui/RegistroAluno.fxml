<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.effect.SepiaTone?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="277.0" prefWidth="610.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.RegistroAlunoController">
   <children>
      <ToolBar prefHeight="40.0" prefWidth="200.0">
        <items>
          <Button fx:id="btRegistrar" mnemonicParsing="false" onAction="#onBtRegistrar" text="Registrar">
               <effect>
                  <SepiaTone />
               </effect></Button>
            <Button fx:id="btDeletar" mnemonicParsing="false" onAction="#onBtDeletar" text="Deletar">
               <effect>
                  <SepiaTone />
               </effect></Button>
            <Button fx:id="btAtualizar" mnemonicParsing="false" onAction="#onBtAtualizar" text="Atualizar">
               <effect>
                  <SepiaTone />
               </effect></Button>
            <Button fx:id="btNotas" mnemonicParsing="false" onAction="#onBtNotas" text="Consultar Notas">
               <effect>
                  <SepiaTone />
               </effect></Button>
            <HBox prefHeight="22.0" prefWidth="200.0" />
            <Button fx:id="btLogOut" mnemonicParsing="false" onAction="#onActionBtLogOut" text="Log Out">
               <effect>
                  <SepiaTone />
               </effect>
               <font>
                  <Font name="Lucida Console" size="12.0" />
               </font>
            </Button>
        </items>
      </ToolBar>
      <TableView fx:id="tableView" prefHeight="200.0" prefWidth="200.0" style="-fx-background-image:url(/background1/fundo_login3.jpg);">
        <columns>
          <TableColumn fx:id="colunaRa" prefWidth="89.0" text="RA" />
          <TableColumn fx:id="colunaNome" prefWidth="298.0" text="Nome" />
            <TableColumn fx:id="colunaTurma" prefWidth="82.0" text="Turma" />
        </columns>
      </TableView>
   </children>
</VBox>
