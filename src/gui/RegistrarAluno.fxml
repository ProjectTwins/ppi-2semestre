<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.SepiaTone?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.RegistrarAlunoController">
   <children>
      <Pane prefHeight="295.0" prefWidth="372.0" style="-fx-background-image: url(/background1/fundo_login7.jpg);">
         <children>
            <Button fx:id="salvar" layoutX="186.0" layoutY="235.0" mnemonicParsing="false" onAction="#onActionSalvar" text="Salvar">
               <font>
                  <Font name="Lucida Console" size="12.0" />
               </font>
               <effect>
                  <SepiaTone />
               </effect>
            </Button>
            <Button fx:id="cancelar" layoutX="254.0" layoutY="235.0" mnemonicParsing="false" onAction="#onActionCancelar" text="Cancelar">
               <font>
                  <Font name="Lucida Console" size="12.0" />
               </font>
               <effect>
                  <SepiaTone />
               </effect>
            </Button>
            <TextField fx:id="ra" editable="false" layoutX="105.0" layoutY="165.0" promptText="RA" />
            <Text fill="#191919" layoutX="73.0" layoutY="182.0" strokeType="OUTSIDE" strokeWidth="0.0" text="RA:">
               <font>
                  <Font name="Lato Black" size="12.0" />
               </font>
            </Text>
            <TextField fx:id="turma" layoutX="105.0" layoutY="123.0" promptText="Turma" />
            <TextField fx:id="nome" layoutX="105.0" layoutY="81.0" promptText="Nome" />
            <Text fill="#161616" layoutX="51.0" layoutY="140.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Turma:">
               <font>
                  <Font name="Lato Black" size="12.0" />
               </font>
            </Text>
            <Text fill="#161616" layoutX="55.0" layoutY="98.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Nome:">
               <font>
                  <Font name="Lato Black" size="12.0" />
               </font>
            </Text>
            <Label layoutX="118.0" layoutY="37.0" text="Registrar Aluno" textAlignment="CENTER" textFill="#161616">
               <font>
                  <Font name="Corbel Bold" size="18.0" />
               </font>
            </Label>
         </children>
      </Pane>
   </children>
</VBox>
